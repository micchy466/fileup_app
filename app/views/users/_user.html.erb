<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Image:</strong>
    <% if user.images.length != 0 %>
      <% user.images.each do |image| %>
        <%= image_tag image.variant(resize: "200x200"), width: 100 %>  
      <% end %>
    <% end %>
  </p>

</div>
